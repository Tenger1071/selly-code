static void Main(string[] args)
        {
            int sayi = Convert.ToInt32(Console.ReadLine());
            if(sayi<0)
            {
                Console.WriteLine("Negatif");
            }
            else if(sayi==0)
            {
                Console.WriteLine("Sayı Sıfır");
            }
            else
            {
                Console.WriteLine("Pozitif");
            }
            Console.ReadKey();
        }
